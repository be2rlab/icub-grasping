<!-- instantiation of
     icub-main/app/iCubCluster/conf/cluster-config.xml
     for iCubLisboa01 -->

<cluster name="icub01" user="icub">

<nameserver namespace="/icub" node="icub-laptop"> </nameserver>

<!-- specify nodes, if you want to enable display on desktop please specify which desktop to use
this is usually the value of the DISPLAY variable when logged on -->
<node display=":0">icub-laptop</node>
<node>pc104</node>
<node>icubbrain1</node>
<node>icubbrain2</node>
<node>icub-cuda</node>

</cluster>

